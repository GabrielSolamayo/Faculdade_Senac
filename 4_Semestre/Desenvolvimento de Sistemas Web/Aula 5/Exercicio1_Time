    <script type="text/javascript">

    function receberTime() // PROMT
        {
            var time=prompt("Digite o nome do seu time: " , " ex: Palmeiras ");
            
        
        if(time=="Sao paulo"){
            alert("Parabens voce torce para o melhor time do mundo: " + time)}

        else { alert("mude para o melhor ") }
        }
    
    </script>


    </script>
</head>
<body>
    
    <input type="button" onclick="receberTime()" value="Digitar o time "/><br/><br/>
  
</body>
